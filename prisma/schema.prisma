// This is your Prisma schema file
// Learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider        = "prisma-client-js"
  previewFeatures = ["driverAdapters"]
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Expense {
  id    Int    @id @default(autoincrement())
  date  DateTime
  title String
  amount Float
  tag   String
}

model Income {
  id     Int      @id @default(autoincrement())
  date   DateTime
  title  String
  amount Float
  tag    String
}